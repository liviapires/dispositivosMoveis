{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6"
     ],
     "target": "es5",
     "module": "commonjs",
     "outDir": "./build",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "esModuleInterop": true,  
     "forceConsistentCasingInFileNames": true,
     "strict": true, 
     "skipLibCheck": true ,
     "sourceMap": true
  }
}